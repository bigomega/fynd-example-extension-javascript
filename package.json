{
  "name": "groot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest --updateSnapshot --projects src app --no-cache --detectOpenHandles --json --outputFile=./coverage/coverage.json"
  },
  "jest": {
    "projects": [
      "src/jest.config.js",
      "app/jest.config.js"
    ]
  },
  "dependencies": {
    "axios": "^1.2.2",
    "core-js": "^3.8.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "url-join": "^5.0.0",
    "vue": "^3.2.13",
    "vue-router": "^4.1.6",
    "window-or-global": "^1.0.1",
    "express": "^4.18.1",
    "fdk-client-javascript": "git+https://github.com/gofynd/fdk-client-javascript.git#0.1.25",
    "fdk-extension-javascript": "git+https://github.com/gofynd/fdk-extension-javascript.git#v0.5.3",
    "ioredis": "^5.2.2",
    "convict": "^6.2.3",
    "cookie-parser": "^1.4.6"
  },
  "devDependencies": {
    "babel-polyfill": "6.26.0",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off",
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
